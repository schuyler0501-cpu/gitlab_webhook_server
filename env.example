# 服务器配置
PORT=3000
NODE_ENV=development

# GitLab Webhook 配置
GITLAB_WEBHOOK_SECRET=your_webhook_secret_here

# 数据库配置
# 数据库类型: mysql, postgresql (默认: mysql)
DB_TYPE=mysql

# MySQL 配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=password
DB_NAME=gitlab_webhook
DB_CHARSET=utf8mb4
DB_TIMEZONE=Asia/Shanghai

# PostgreSQL 配置（当 DB_TYPE=postgresql 时使用）
# DB_HOST=localhost
# DB_PORT=5432
# DB_USER=postgres
# DB_PASSWORD=password
# DB_NAME=gitlab_webhook
# DB_SSLMODE=disable
# DB_TIMEZONE=Asia/Shanghai

# 日志配置
LOG_LEVEL=info

# 工作池配置
WORKER_POOL_WORKERS=10
WORKER_POOL_QUEUE_SIZE=1000

# 限流配置
RATE_LIMIT=100
RATE_LIMIT_WINDOW=1m

# GitLab API 配置（用于历史数据导入）
GITLAB_BASE_URL=https://gitlab.com
GITLAB_TOKEN=your_gitlab_token_here

